{
  "manifest_version": 3,
  "name": "DSA Mentor",
  "version": "0.2",
  "permissions": ["storage", "tabs"],
  "action": { "default_popup": "popup.html" },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}